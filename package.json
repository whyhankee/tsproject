{
	"name": "tsproject",
	"description": "Typescript starter project",
	"version": "0.0.1",
	"main": "dist/index.js",
	"scripts": {
		"start": "node ./dist/index.js",
		"dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
		"build": "esbuild ./src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js",
		"check": "pnpm run lint && pnpm run test --run",
		"lint": "eslint --fix",
		"test": "vitest"
	},
	"packageManager": "^pnpm@8.14.3",
	"devDependencies": {
		"@types/node": "20.11.7",
		"@typescript-eslint/eslint-plugin": "^6.19.1",
		"@typescript-eslint/parser": "6.19.1",
		"esbuild": "0.19.12",
		"eslint": "8.56.0",
		"eslint-plugin-simple-import-sort": "10.0.0",
		"ts-node-dev": "2.0.0",
		"typescript": "5.3.3",
		"vitest": "1.2.2"
	},
	"keywords": [],
	"author": "Paul Wijhenke <paul@paulsplace.nl>",
	"license": "ISC"
}
