{
	"name": "tsproject",
	"description": "Typescript starter project",
	"version": "0.0.1",
	"type": "module",
	"main": "dist/index.mjs",
	"scripts": {
		"start": "node ./dist/index.mjs",
		"dev": "tsx --watch ./src/index.ts",
		"build": "esbuild ./src/index.ts --bundle --platform=node --target=node20 --format=esm --outfile=dist/index.mjs",
		"lint": "eslint \"./**/*.{ts,js}\"",
		"format": "prettier --write \"./**/*.{ts,js,json,md}\"",
		"test": "vitest --coverage",
		"test:ci": "pnpm run lint && pnpm run test --run",
		"prepare": "husky"
	},
	"packageManager": "^pnpm@8.14.3",
	"devDependencies": {
		"@stylistic/eslint-plugin": "1.5.4",
		"@types/node": "20.11.7",
		"@typescript-eslint/eslint-plugin": "6.19.1",
		"@typescript-eslint/parser": "6.19.1",
		"@vitest/coverage-v8": "1.2.2",
		"esbuild": "0.19.12",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-simple-import-sort": "10.0.0",
		"husky": "9.0.11",
		"install": "0.13.0",
		"prettier": "3.2.5",
		"tsx": "4.7.1",
		"typescript": "5.3.3",
		"vitest": "1.2.2"
	},
	"keywords": [
		"starter",
		"typescript"
	],
	"author": "Paul Wijhenke <paul@paulsplace.nl>",
	"license": "ISC"
}
